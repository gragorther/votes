generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "./.dev.db"
}


enum LikeType {
  Like
  Dislike
}

model User {
  Id Int @default(autoincrement()) @id
  Name String
  Username String @unique
  likes Like[]
}

model Like {
  Id Int @default(autoincrement()) @id
  Url String
  Actor String
  Type LikeType
  userId Int
  user User @relation(fields: [userId], references: [Id])
}